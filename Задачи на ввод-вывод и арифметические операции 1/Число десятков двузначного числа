//Число десятков двузначного числа
//Дано двузначное число. Найдите число десятков в нем.
//
//Входные данные
//
//Вводится двузначное число.
//
//Выходные данные
//
//Выведите ответ на задачу.
//
//Sample Input:
//
//76
//Sample Output:
//
//7

const readline = require('readline');

const r1 = readline.createInterface({
  input: process.stdin,
  output: process.stdout
});

r1.on('line', (n1) => {
    n = parseInt(n1);
    console.log(calcFirstFigure(n));
});

function calcFirstFigure(n){
    while(Math.abs(n) > 10){
          n = Math.abs(parseInt(n / 10));
    }

    return n;
}